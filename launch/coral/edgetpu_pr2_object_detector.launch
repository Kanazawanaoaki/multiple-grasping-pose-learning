<launch>
  <arg name="INPUT_IMAGE"/>
  
  <!-- add my models -->
  <!-- <arg name="model_file" default="$(find multiple_grasping_pose_learning)/models/pr2/pr2_look_around_dataset_20220114-aug_pr2_manip_n2_try/output_tflite_graph_edgetpu.tflite"/> -->
  <arg name="model_file" default="$(find multiple_grasping_pose_learning)/models/pr2/pr2_look_around_dataset_20220114-aug_pr2_manip_n2_try/my_object_detection.tflite"/>
  <arg name="label_file" default="$(find multiple_grasping_pose_learning)/models/pr2/pr2_look_around_dataset_20220114-aug_pr2_manip_n2_try/labels.txt"/>

  <node name="edgetpu_object_detector"
        pkg="coral_usb" type="edgetpu_object_detector.py"
        output="screen" respawn="true">
    <remap from="~input" to="$(arg INPUT_IMAGE)" />
    <rosparam subst_value="true" >
      model_file: $(arg model_file)
      label_file: $(arg label_file)
    </rosparam>
  </node>
</launch>
